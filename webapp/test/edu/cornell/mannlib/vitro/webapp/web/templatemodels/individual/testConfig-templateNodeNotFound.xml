<?xml version="1.0" encoding="ISO-8859-1"?>

<!--
	No template node. Should throw an exception.  
-->

<list-view-config>
    <query-select>
        SELECT <collated> ?subclass </collated>
               ?object
        WHERE {
            ?subject ?property ?object
            <collated>
            ?object a ?subclass.
            </collated>
        }  
    </query-select>

    <query-construct>
        CONSTRUCT {
            ?subject ?property ?object .
        } WHERE {
            ?subject ?property ?object 
        }
    </query-construct>
    
</list-view-config>
